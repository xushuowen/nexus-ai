# Nexus AI - API Keys and Configuration
# Copy this to .env and fill in your API keys

# LLM Providers
# GitHub Models - free, no credit card (https://github.com/marketplace/models)
GITHUB_TOKEN=your_github_personal_access_token_here

# Fallback LLMs (optional)
GROQ_API_KEY=your_groq_api_key_here
GEMINI_API_KEY=your_gemini_api_key_here
OPENAI_API_KEY=
ANTHROPIC_API_KEY=

# Telegram Bot (optional - get token from @BotFather)
TELEGRAM_BOT_TOKEN=

# Email SMTP (optional)
SMTP_HOST=
SMTP_PORT=587
SMTP_USER=
SMTP_PASS=

# Budget override (optional)
NEXUS_DAILY_LIMIT=50000
